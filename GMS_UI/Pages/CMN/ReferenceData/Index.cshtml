@page
@model GMS_UI.Pages.CMN.ReferenceData.IndexModel
@{
    ViewData["Title"] = "Reference Data";
}

<style>
    /* Page Title */
    h2.mb-4 {
        color: #5f9ea0;
        font-weight: 600;
    }

    /* Tabs */
    .nav-tabs .nav-link {
        color: #5f9ea0;
        font-weight: 500;
    }

    .nav-tabs .nav-link.active {
        background: linear-gradient(135deg, #7fb3b5 0%, #5f9ea0 100%);
        color: white;
        border-color: #5f9ea0;
    }

    .nav-tabs .nav-link:hover {
        border-color: #5f9ea0;
        color: #4a7a7c;
    }

    /* Accordion */
    .accordion-button.accordionStyle {
        background: linear-gradient(135deg, #e8f4f5 0%, #d4e9ea 100%);
        color: #2c5f61;
        font-weight: 500;
    }

    .accordion-button.accordionStyle:not(.collapsed) {
        background: linear-gradient(135deg, #7fb3b5 0%, #5f9ea0 100%);
        color: white;
    }

    .accordion-button.accordionStyle:focus {
        box-shadow: 0 0 0 0.25rem rgba(95, 158, 160, 0.25);
    }

    /* Reference Headers */
    .headerReference {
        background: linear-gradient(135deg, #7fb3b5 0%, #5f9ea0 100%);
        color: white;
        padding: 0.75rem 1rem;
        border-radius: 8px 8px 0 0;
    }

    .buttonReference {
        background: white;
        color: #5f9ea0;
        border: 2px solid white;
        font-weight: 500;
    }

    .buttonReference:hover {
        background: #f0f9fa;
        color: #4a7a7c;
        transform: translateY(-1px);
    }

    /* AG-Grid */
    .ag-theme-balham .ag-header {
        background: linear-gradient(to bottom, #f8f9fa, #e9ecef);
        border-bottom: 2px solid #5f9ea0;
        font-weight: 600;
    }

    .ag-theme-balham .ag-row:hover {
        background-color: #e8f4f5 !important;
    }

    .ag-theme-balham .ag-row-selected {
        background-color: #d4e9ea !important;
    }

    /* Modal Headers */
    .modal-header {
        background: linear-gradient(135deg, #7fb3b5 0%, #5f9ea0 100%);
        color: white;
        border-bottom: none;
    }

    .modal-header h2 {
        color: white;
        font-weight: 600;
        font-size: 1.5rem;
        margin: 0;
    }

    .modal-header .close {
        color: white;
        opacity: 0.9;
        font-size: 1.5rem;
        cursor: pointer;
        background: none;
        border: none;
    }

    .modal-header .close:hover {
        opacity: 1;
    }

    /* Success Buttons */
    .btn-success {
        background: linear-gradient(135deg, #7fb3b5 0%, #5f9ea0 100%);
        border: none;
        font-weight: 500;
        box-shadow: 0 2px 4px rgba(95, 158, 160, 0.2);
        transition: all 0.3s ease;
    }

    .btn-success:hover {
        background: linear-gradient(135deg, #5f9ea0 0%, #4a7a7c 100%);
        transform: translateY(-1px);
        box-shadow: 0 4px 8px rgba(95, 158, 160, 0.3);
    }
</style>

<div class="container-gms">

