<!-- Gender Modals -->
<div id="genderNewModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="genderNewModalLabel" aria-hidden="true" data-bs-keyboard="false" data-bs-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" role="document">
            <div class="modal-header page-card-header">
                <h2>Add Gender</h2>
                <span class="close" data-bs-dismiss="modal">&times;</span>
            </div>
            <div class="modal-body">
                <div class="form-section">
                    <div class="row">
                        <div class="col">
                            <div class="mb-3">
                                <label for="gender">Gender</label>
                                <input type="text" class="form-control inputsmmodal" id="gender" placeholder="Gender">
                                <span id="validateGender" class="text-danger"></span>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col">
                            <div class="mb-3">
                                <label for="comment">Comment</label>
                                <textarea rows="4" class="form-control inputsmmodal" placeholder="Comment" id="commentGender"></textarea>
                            </div>
                        </div>
                    </div>
                </div>

                

            </div>
            <div class="modal-footer">
                <button class="btn btn-sm btn-outline-success" onclick="crudGender(1)"> <i class="bi bi-save"></i> Save</button>
            </div>
        </div>
    </div>

</div>

<div id="editGender" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="editGenderLabel" aria-hidden="true" data-bs-keyboard="false" data-bs-backdrop="static">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" role="document">
            <div class="modal-header page-card-header">
                <h2>Edit Gender</h2>
                <span class="close" data-bs-dismiss="modal">&times;</span>
            </div>
            <div class="modal-body">
                <div class="form-section">
                    <div class="row">
                        <div class="col">
                            <div class="mb-3">
                                <label for="gender">Gender</label>
                                <input type="text" class="form-control inputsmmodal" id="genderEdit" placeholder="Gender">
                                <span id="validateGenderEdit" class="text-danger"></span>
                            </div>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col">
                            <div class="mb-3">
                                <label for="comment">Comment</label>
                                <textarea rows="4" class="form-control inputsmmodal" placeholder="Comment" id="commentGenderEdit"></textarea>
                            </div>
                        </div>
                    </div>
                </div>
                <input type="hidden" id="genderidEdit" />
            </div>
            <div class="modal-footer">
                <button class="btn btn-sm btn-outline-success" onclick="crudGender(2)"> <i class="bi bi-save"></i> Save</button>
            </div>
        </div>
    </div>

</div>

