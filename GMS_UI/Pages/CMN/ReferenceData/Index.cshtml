<!-- Gender Modals -->
<div id="genderNewModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="genderNewModalLabel" aria-hidden="true" data-bs-keyboard="false" data-bs-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content" role="document">
            <div class="modal-header">
                <h2>Add Gender</h2>
                <span class="close" data-bs-dismiss="modal">&times;</span>
            </div>
            <div class="modal-body">
                <div class="form-group position-relative">
                    <label class="floating-label" for="gender">Gender</label>
                    <input type="text" class="form-control pt-4" id="gender" placeholder="Gender">
                    <span id="validateGender" class="text-danger"></span>
                </div>

                <div class="form-group position-relative">
                    <label class="floating-label" for="comment">Comment</label>
                    <textarea class="form-control pt-4" placeholder="Comment" id="commentGender"></textarea>
                </div>

            </div>
            <div class="modal-footer">
                <button class="btn btn-sm btn-success" onclick="crudGender(1)"> Save</button>
            </div>
        </div>
    </div>

</div>

<div id="editGender" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="editGenderLabel" aria-hidden="true" data-bs-keyboard="false" data-bs-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content" role="document">
            <div class="modal-header">
                <h2>Edit Gender</h2>
                <span class="close" data-bs-dismiss="modal">&times;</span>
            </div>
            <div class="modal-body">
                <div class="form-group position-relative">
                    <label class="floating-label" for="gender">Gender</label>
                    <input type="text" class="form-control pt-4" id="genderEdit" placeholder="Gender">
                    <span id="validateGenderEdit" class="text-danger"></span>
                </div>

                <div class="form-group position-relative">
                    <label class="floating-label" for="comment">Comment</label>
                    <textarea class="form-control pt-4" placeholder="Comment" id="commentGenderEdit"></textarea>
                </div>
                <input type="hidden" id="genderidEdit" />
            </div>
            <div class="modal-footer">
                <button class="btn btn-sm btn-success" onclick="crudGender(2)"> Save</button>
            </div>
        </div>
    </div>

</div>

<div id="deleteGender" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="deleteGenderLabel" aria-hidden="true" data-bs-keyboard="false" data-bs-backdrop="static">
    <div class="modal-dialog">
        <div class="modal-content" role="document">
            <div class="modal-header">
                <h2>Are you sure you want to delete this gender?</h2>
                <span class="close" data-bs-dismiss="modal">&times;</span>
                <input type="hidden" id="genderidDelete" />
            </div>
            <div class="modal-body">
                <div class="row">
                    <dl class="row">
                        <dt class="col-sm-6">
                            Gender:
                        </dt>
                        <dd class="col-sm-6" id="genderD">

